<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <log4net>
    <root>
      <level value="DEBUG"/>
      <appender-ref ref="AdoNetAppender"/>
    </root>
    <appender name="AdoNetAppender" type="log4net.Appender.AdoNetAppender">
      <connectionString name="log4NetConnection" value="Data Source=yopex-pc\mssql2008;Initial Catalog=OsteoYoga;Integrated Security=True" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </appender>
  </log4net>
  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory>
      <property name="connection.connection_string" xdt:Transform="Replace" xdt:Locator="Match(name)">Data Source=yopex-pc\mssql2008;Initial Catalog=OsteoYoga;Integrated Security=True</property>
    </session-factory>
  </hibernate-configuration>
</configuration>
