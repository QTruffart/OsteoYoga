<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OsteoYoga.Domain"
                   namespace="OsteoYoga.Domain.Models">

  <class name="Contact" table="Contact">
    <id column="Id" name="Id" type="Int32" unsaved-value="0">
      <generator class="increment" />
    </id>
    <property name="FullName" column="FullName" not-null="true" />
    <property name="Phone" column="Phone" not-null="false" />
    <property name="Mail" column="Mail" not-null="true" />
    <property name="NetworkType" column="NetworkType" not-null="false" />
    <property name="NetworkId" column="NetworkId" not-null="false" />
    <many-to-one name="Profile" column="ProfileId" class="Profile" foreign-key="FK_Contact_Profile" />
  </class>
</hibernate-mapping>