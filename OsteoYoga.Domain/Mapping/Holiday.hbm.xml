<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OsteoYoga.Domain"
                   namespace="OsteoYoga.Domain.Models">

  <class name="Holiday" table="Holiday">
    <id column="Id" name="Id" type="Int32" unsaved-value="0">
      <generator class="increment" />
    </id>
    <property name="Day" column="Day" not-null="true" />
    <property name="BeginHour" type="TimeAsTimeSpan" column="BeginHour" not-null="true" />
    <property name="EndHour" type="TimeAsTimeSpan" column="EndHour" not-null="true" />
  </class>
</hibernate-mapping>