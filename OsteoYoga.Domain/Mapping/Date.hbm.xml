<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="OsteoYoga.Domain"
                   namespace="OsteoYoga.Domain.Models">

  <class name="Date" table="Date">
    <id column="Id" name="Id" type="Int32" unsaved-value="0">
      <generator class="increment" />
    </id>
    <property name="Day" column="Day" not-null="true" />
    <many-to-one name="Office" column="OfficeId" class="Office" foreign-key="FK_Date_Office" />
    <many-to-one name="Contact" column="ContactId" class="Contact" foreign-key="FK_Date_Contact" />
    <many-to-one name="Duration" column="DurationId" class="Duration" foreign-key="FK_Date_Duration" />
    <many-to-one name="Contact" column="ContactId" class="Contact" foreign-key="FK_Date_Contact" />
  </class>
</hibernate-mapping>